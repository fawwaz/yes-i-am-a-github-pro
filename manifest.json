{
  "name": "Github Pro",
  "version": "1.0",
  "author": "Fawwaz Muhammad",
  "manifest_version": 2,
  "description": "Put pro-badge on your github profile",
  "homepage_url": "http://www.github.com/fawwaz/yes-i-am-a-github-pro",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://github.com/*", "https://github.com/*"],
      "js": ["resources/jquery.min.js", "src/github-pro.js"]
    }
  ],
  "web_accessible_resources": ["resources/*"]
}
